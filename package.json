    "@tarojs/plugin-platform-qq": "3.6.17",
    "@tarojs/plugin-platform-swan": "3.6.17",
    "@tarojs/plugin-platform-tt": "3.6.17",
    "@tarojs/plugin-platform-weapp": "3.6.17",
    "@tarojs/react": "3.6.17",
    "@tarojs/runtime": "3.6.17",
    "@tarojs/shared": "3.6.17",
    "@tarojs/taro": "3.6.17",
    "ahooks": "^3.7.8",
    "axios": "^1.5.1",
    "axios-miniprogram-adapter": "^0.3.5",
    "dayjs": "^1.11.10",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intl": "^6.5.1",
    "wx-server-sdk": "^2.6.3",
    "x-star-editor": "^0.3.0",
    "x-star-query": "^0.0.1",
    "x-star-utils": "^0.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
