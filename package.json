    "prettier-plugin-organize-imports": "^3.2.4",
    "react-refresh": "^0.11.0",
    "sass": "^1.64.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0",
    "webpack": "5.78.0"
  }
}
